--pico-8 builtins

function _init()
  -- init bg
  bgs = {
    {sprite=77,vx=0,vy=0.2},
    {sprite=78,vx=0.2,vy=0.2}
  }
  bg = rand_choice(bgs)
end

function _update()
	
end

function _draw()
  -- cls()
  draw_bg()
end
