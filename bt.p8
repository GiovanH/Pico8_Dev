-- initialization
function _init()
 -- initialize high score
 high_score = 0
 if (cartdata("gio_bf")) then
  high_score = dget(0)
 else
  dset(0,high_score)
 end
 
 -- menuitem to reset high score
 menuitem(1,"reset high",function()
   high_score = 0
   dset(0,high_score)
  end)
 
 -- debug: double difficulty
 menuitem(2,"difficulty+",function()
   game_tick *= 2
  end)
	-- init_menu()
 init_game()
end

function init_menu()
 state = 0 --menu state
end

function init_game()
 --init_game_drawing
 layer_1 = {}
 layer_2 = {}
 --init game
 state = 1 --game state
 game_tick = 0
 score = 0
 lives = 3
 --delay must be divisible
 --by scroll_spd inc in binary!
 max_mine_delay=60 
 difficulty_interval = 4.75
 scroll_spd = 0.25
 --init actors
	actors = {}
	player = new_player(64, 32)
	new_minedropper()
	for i = 0,20 do
	 new_star(flr(rnd(127)), flr(rnd(127)))
	end
end

-->8
-- updates
function _update()
 if (state==0) update_menu()
	if (state==1) update_game()
end

function update_menu()
 --nothing
end

function update_game()
 game_tick += 0.25
 --10 points per second
 if (game_tick % 7.5 == 0) score += 10

 if (game_tick > difficulty_interval) then
  if (max_mine_delay > 35) then
   max_mine_delay -= 4 
  else
   scroll_spd += 0.125
   max_mine_delay = 32
  end
 -- change must be x=1/scroll_spd
 -- to be prevent desync
  difficulty_interval *= 2 
 end
 
 if (score > high_score) then
  high_score = score
  dset(0,high_score)
 end
 foreach(actors, update_physics)
 for a in all(actors) do
  a.u_tick(a)
 end
end

function update_physics(actor)
 -- skip non physics objects
 if not actor.physics then
  return
 end
 -- we do this at the beginning, not the end, so that draw can check them
 actor.dx, actor.dy = 0,0
 
 actor.vy += actor.gravity

 actor.dx += actor.vx
 actor.dy += actor.vy
 
 actor.vx -= actor.fx*actor.vx
 actor.vy -= actor.fy*actor.vy
 
 actor.x += actor.dx
 actor.y += actor.dy
 
 actor.x += scroll_spd
 
-- actor.x = flr(actor.x*10)/10
end

-->8
-- drawing

function _draw()
	if (state==0) draw_menu()
	if (state==1) draw_game()
end

function draw_menu()
 
end

function draw_game()
 cls()
 for layer in all({layer_2, layer_1}) do
  for a in all(layer) do
   a.d_tick(a)
  end
  foreach(layer, draw_actor)
 end
 
 draw_waves()
 draw_hud()
 --draw_debug()
end

function draw_hud()
 for i = 1,lives do
  spr(19,(8*i)+44,1)
 end
 print("score: " .. score,1,1,7)
 printl("high: " .. high_score,128,1,7)
end

function draw_waves()
 if (wave_offset == nil) wave_offset = 0
 wave_offset += scroll_spd
 wave_offset %= 8
 for x=-1,16 do
 spr(008,(8*x)+wave_offset,120)
 end
end

function draw_debug()
 local x = 0
 local y = 128-32
 local strs = {game_tick, difficulty_interval, max_mine_delay, scroll_spd}
 for s in all(strs) do
  print(s, x, y)
  y += 8
 end
end

function draw_actor(actor)
 if not actor.draw then
  return
 end
 local fx = (actor.dx > 0)
 spr(actor.sprite + actor.frame, actor.x, actor.y, 1, 1, fx)
end
