-- constructors

function new_actor(x, y)
 a = {}
 a.name = "generic"
 a.physics = false
 a.draw = true
 a.sprite = 0
 a.frame = 0
 a.frames = 1
 -- position
 a.x = x
 a.y = y
 -- lambda
 a.dx = 0
 a.dy = 0
 -- velocity
 a.vx = 0
 a.vy = 0
 -- friction coefficient
 a.fx = 0.06
 a.fy = 0.06
 a.gravity = 0.15
 a.u_tick = function(this) end
 a.d_tick = function(this) end
 add(actors,a)
 a.rm=function(t)
  del(actors,t)
 end
 return a
end


function new_respawner(x,y)
 a = new_actor(x,y)
 a.name = "respawner"
 a.ticks = 30
 a.tick = 0
 a.u_tick = function(t)
  t.tick += 1
   if (btn(0)) t.x -= 2
   if (btn(1)) t.x += 2
   if (btn(2)) t.y -= 2
   if (btn(3)) t.y += 2
  
  if (t.tick == t.ticks) then
   player = new_player(t.x,t.y)
   t:rm()
  end
 end
 add(layer_1,a)
 a.rm=function(t)
  del(actors,t)
  del(layer_1,t)
 end
 return a
end

function new_player(x, y)
 a = new_actor(x, y)
 a.name = "player"
 a.physics = true
 a.sprite = 1
 a.frames = 1
 a.balloons = 2
 a.strafe_spd = 0.3
 a.ffall_spd = 0
 a.jump_spd = 2.5
 a.d_tick = function(t) 
   local balloon_sprite = 16
   if (t.balloons == 2) balloon_sprite = 18
   if (t.balloons == 1) balloon_sprite = 17
   spr(balloon_sprite, t.x, t.y-8)
  end
 a.u_tick = function(t)
  -- controls
  if (t.balloons > 0) then
   if (btn(0)) player.vx -= player.strafe_spd
   if (btn(1)) player.vx += player.strafe_spd
   if (btnp(2) or btnp(4)) player.vy -= player.jump_spd
   if (btn(3)) player.vy += player.ffall_spd
  end
  -- screen wrap
  if (t.x > 127) t.x = 127
  if (t.x < 1) t.x = 1
 
  --ciel
  if (t.y < 2) t.y = 2
 
  --bottom
  if (t.y > 127) then
   sfx(sound.splash)
   if (lives > 0) then
    new_respawner(64,32)
    t:rm()
    lives -= 1
   else
    init_game()
   end
   return
  end  
  
  local m = sprite_collides(t,{"mine"},0)
  if m then
   t.balloons = 0
   m:rm()
   sfx(sound.hurt)
  end
  m = sprite_collides(t,{"mine"},-8)
  if m then
   t.balloons -= 1
   m:rm()
   sfx(sound.pop)
  end
 end
 add(layer_1,a)
 a.rm=function(t)
  del(actors,t)
  del(layer_1,t)
 end
 return a
end

function new_minedropper()
 e = new_actor(0,0)
 e.name = "minedropper"
 e.sprite = 024
 e.countdown=max_mine_delay
 e.u_tick = function(t)
   t.x=-8
   t.y=2+flr(4*rnd(28))
   t.countdown -= 1
   if (t.countdown <= 0) then
    t.countdown = max_mine_delay
    new_mine(t.x,t.y)
   end
  end
 add(layer_1,e)
 e.rm=function(t)
  del(actors,t)
  del(layer_1,t)
 end
end

function new_mine(x,y)
 m = new_actor(x,y)
 m.name = "mine"
 m.sprite = 024
 m.physics = true
 m.gravity = 0
 m.fx = 0
 add(layer_1,m)
 m.u_tick=function(t)
   if (t.x > 128) t:rm()
  end
 m.d_tick=function(t)
   --print(t.x,t.x,t.y-8,1)
  end
 m.rm=function(t)
  del(actors,t)
  del(layer_1,t)
 end
 return m
end

function new_star(x,y)
 a.name = "star"
 max_frames = 20
 pause = 8
 a = new_actor(x,y)
 a.draw = false
 a.physics = true
 a.gravity = 0
 a.fx = 0
 a.vx = -0.1875
 a.frame = flr(rnd(max_frames))
 a.frames = max_frames
 a.u_tick = function(t)
   t.frame += 1
   t.frame %= t.frames
   if (t.x > 127) then
    t.x = 0
    t.y = flr(rnd(127))
   end
  end
 a.d_tick = function(t)
  if (1 <= t.frame and t.frame < pause) then
   return
  end
  clr = 7
  if (t.frame == 0 or t.frame == pause-1) clr = 13
  pset(t.x,t.y,clr)
  --print(t.x, t.x,t.y)
 end
 add(layer_2,a)
 a.rm=function(t)
  del(actors,t)
  del(layer_2,t)
 end
 return a
end
