-- utility

function sprite_collides(s,filter,y_offset)
 for other in all(actors) do
  if table_contains(filter,other.name) then
   local x1 = s.x
   local y1 = s.y + y_offset
   local x2 = other.x
   local y2 = other.y
   if  (x1+8 > x2 and x2+8 > x1)
   and (y1+8 > y2 and y2+8 > y1)
   then
    return other
   end
  end
  --else break
 end
 return false
end

function table_contains(table, element)
  for key, value in pairs(table) do
    if value == element then
      return true
    end
  end
  return false
end

function printl(string,x,y,c)
 print(string,x-(4*#string),y,c)
end
